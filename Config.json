{
    "end_with_error": false,
    "default_parameters": {
        "higher_temp": 25,
        "lower_temp": 4,
        "cycle_duration_hours": 12,
        "experiment_duration_days": 30,
        "logging_interval": 60,
        "end_cycle_earlier": false,
        "tasks": [
            {
                "class": "TemperatureCycle",
                "use_defaults": [
                    {
                        "param": "cycle_duration_hours",
                        "as": "interval_hours"
                    },
                    {
                        "param": "experiment_duration_days",
                        "as": "period_days"
                    },
                    {
                        "param": "lower_temp"
                    },
                    {
                        "param": "higher_temp"
                    }
                ]
            },
            {
                "class": "TemperatureLogger",
                "use_defaults": [
                    {
                        "param": "logging_interval",
                        "as": "interval"
                    },
                    {
                        "param": "experiment_duration_days",
                        "as": "period_days"
                    }
                ]
            }
        ]
    },
    "current_status": {
        "end_with_error": false
    }
}